}

.sortableTable tr.even.gradeA td.sorting_3 {
	background-color: #e2ffe2;
}

.sortableTable tr.odd.gradeC td.sorting_1 {
	background-color: #c4c4ff;
}

.sortableTable tr.odd.gradeC td.sorting_2 {
	background-color: #d1d1ff;
}

.sortableTable tr.odd.gradeC td.sorting_3 {
	background-color: #d1d1ff;
}

.sortableTable tr.even.gradeC td.sorting_1 {
	background-color: #d5d5ff;
}

.sortableTable tr.even.gradeC td.sorting_2 {
	background-color: #e2e2ff;
}

.sortableTable tr.even.gradeC td.sorting_3 {
