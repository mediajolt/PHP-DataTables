.sortableTable tr.even td.sorting_2 {
	background-color: #ecf2f6;
}

.sortableTable tr.even td.sorting_3 {
	background-color: #ecf2f6;
}

/* For the Conditional-CSS grading rows */
/*
 	Colour calculations (based off the main row colours)
  Level 1:
		dd > c4
		ee > d5
	Level 2:
	  dd > d1
	  ee > e2
 */
.sortableTable tr.odd.gradeA td.sorting_1 {
	background-color: #c4ffc4;
}

.sortableTable tr.odd.gradeA td.sorting_2 {
	background-color: #d1ffd1;
}

.sortableTable tr.odd.gradeA td.sorting_3 {
