}

.sortableTable tr.even.gradeX td.sorting_3 {
	background-color: #ffe2e2;
}

.sortableTable .sortableTable tr.odd.gradeU td.sorting_1 {
	background-color: #c4c4c4;
}

.sortableTable tr.odd.gradeU td.sorting_2 {
